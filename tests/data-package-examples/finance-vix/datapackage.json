{
  "profile": "tabular-data-package",
  "name": "finance-vix",
  "title": "VIX - CBOE Volatility Index",
  "homepage": "http://www.cboe.com/micro/VIX/",
  "version": "0.0.1",
  "sources": [{
    "title": "CBOE VIX Page",
    "path": "http://www.cboe.com/micro/vix/historical.aspx"
  }],
  "resources": [{
    "profile": "tabular-data-resource",
    "name": "vix-daily",
    "path": "data/vix-daily.csv",
    "format": "csv",
    "mediatype": "text/csv",
    "schema": {
      "fields": [{
          "name": "Date",
          "type": "date",
          "format": "%m/%d/%Y"
        },
        {
          "name": "VIXOpen",
          "type": "number"
        },
        {
          "name": "VIXHigh",
          "type": "number"
        },
        {
          "name": "VIXLow",
          "type": "number"
        },
        {
          "name": "VIXClose",
          "type": "number"
        }
      ]
    }
  }]
}
